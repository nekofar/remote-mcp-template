{
  "name": "remote-mcp-template",
  "version": "1.0.0-alpha.26",
  "description": "Template for a Model Context Protocol (MCP) server.",
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "pnpx wrangler@4.16.1 dev --port 8787",
    "build": "pnpx wrangler deploy --dry-run",
    "format": "biome format --write",
    "lint": "biome lint && pnpm run type-check",
    "lint:fix": "biome lint --fix",
    "start": "pnpm run dev",
    "test": "vitest --run",
    "cf-typegen": "wrangler types --include-runtime=false --strict-vars=false --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "type-check": "tsc --noEmit",
    "inspector": "MCP_AUTO_OPEN_ENABLED=false mcp-inspector --transport sse --server-url http://localhost:8787/sse",
    "tunnel": "ngrok http 8787"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "template"
  ],
  "author": "Milad Nekofar <milad@nekofar.com>",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=20.6.0"
  },
  "packageManager": "pnpm@10.26.1",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.25.1",
    "agents": "0.3.0",
    "ai": "6.0.3",
    "hono": "4.11.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@cloudflare/vitest-pool-workers": "0.11.1",
    "@cloudflare/workers-types": "4.20251229.0",
    "@modelcontextprotocol/inspector": "0.18.0",
    "@types/node": "25.0.3",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "wrangler": "4.56.0"
  }
}

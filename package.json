{
  "name": "remote-mcp-template",
  "version": "1.0.0-alpha.37",
  "description": "Template for a Model Context Protocol (MCP) server.",
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "pnpx wrangler@4.16.1 dev --port 8787",
    "build": "pnpx wrangler deploy --dry-run",
    "format": "biome format --write",
    "lint": "biome lint && pnpm run type-check",
    "lint:fix": "biome lint --fix",
    "start": "pnpm run dev",
    "test": "vitest --run",
    "cf-typegen": "wrangler types --include-runtime=false --strict-vars=false --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "type-check": "tsc --noEmit",
    "inspector": "MCP_AUTO_OPEN_ENABLED=false mcp-inspector --transport sse --server-url http://localhost:8787/sse",
    "tunnel": "ngrok http 8787"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "template"
  ],
  "author": "Milad Nekofar <milad@nekofar.com>",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": "^24",
    "pnpm": "^10"
  },
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.25.3",
    "agents": "0.3.6",
    "ai": "6.0.67",
    "hono": "4.11.7",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@cloudflare/vitest-pool-workers": "0.12.8",
    "@cloudflare/workers-types": "4.20260131.0",
    "@modelcontextprotocol/inspector": "0.19.0",
    "@types/node": "25.1.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "wrangler": "4.61.1"
  }
}
